entrypoint = "main.py"
modules = ["python-3.10"]

hidden = [".pythonlibs"]

[nix]
channel = "stable-23_05"

[deployment]
run = ["sh", "-c", "python main.py"]
deploymentTarget = "gce"
ignorePorts = true

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 41683493
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"

[[ports]]
localPort = 8080
externalPort = 80
